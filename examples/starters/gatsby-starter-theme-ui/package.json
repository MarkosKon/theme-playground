{
  "name": "gatsby-starter-theme-ui",
  "private": true,
  "description": "Gatsby and theme-ui",
  "version": "1.0.0",
  "author": "Markos Konstantopoulos <markoskonstanto@hotmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "clean": "gatsby clean",
    "lint": "eslint --ext js,jsx src",
    "format": "yarn lint --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,json,svg,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/css": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@mdx-js/mdx": "^1.0.21",
    "@mdx-js/react": "^1.0.21",
    "gatsby": "^2.10.4",
    "gatsby-image": "^2.2.3",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-catch-links": "^2.1.0",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-remark-prismjs": "^3.3.0",
    "gatsby-source-filesystem": "^2.1.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-slick": "^0.24.0",
    "rellax": "^1.10.0",
    "remark-slug": "^5.1.2",
    "sal.js": "^0.6.1",
    "slick-carousel": "^1.8.1",
    "theme-ui": "^0.2.0",
    "typeface-lora": "^0.0.72",
    "typeface-oswald": "^0.0.54"
  },
  "devDependencies": {
    "@testing-library/cypress": "^4.0.4",
    "axe-core": "^3.2.2",
    "cypress": "^3.3.2",
    "cypress-axe": "^0.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.9.1"
  }
}
